<?xml version="1.0" encoding="UTF-8"?>
<sdx:platform sdx:vendor="xilinx.com"
              sdx:library="sdx"
              sdx:name="part_3_v1"
              sdx:version="1.0"
              sdx:schemaVersion="1.0"
              xmlns:sdx="http://www.xilinx.com/sdx">
    <sdx:description>
part_3_v1
    </sdx:description>
    <sdx:systemConfigurations sdx:defaultConfiguration="part_3_v1">
        <sdx:configuration sdx:name="part_3_v1"
                           sdx:displayName="part_3_v1"
                           sdx:defaultProcessorGroup="standalone_domain"
                           sdx:runtimes="cpp">
            <sdx:description>part_3_v1</sdx:description>
            <sdx:bootImages sdx:default="standard">
                <sdx:image sdx:name="standard"
                            sdx:bif="part_3_v1/boot/part_3_v1.bif"
