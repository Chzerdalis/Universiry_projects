                            sdx:qemuArguments="part_3_v1/qemu/qemu_args.txt"
                            />
                <sdx:image sdx:name="standalone_domain"
                            sdx:bif="part_3_v1/boot/part_3_v1.bif"
                            sdx:readme="part_3_v1/boot/generic.readme"
                            sdx:qemuArguments="part_3_v1/standalone_domain/qemu/qemu_args.txt"
                            />
            </sdx:bootImages>
            <sdx:processorGroup sdx:name="standalone_domain"
